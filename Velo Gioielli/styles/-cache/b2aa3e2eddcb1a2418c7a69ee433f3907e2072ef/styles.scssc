/* Misc */

.floatnone {float: none;}
.float-left {float:left;}


.clearfix:after {clear:both; content:"."; display:block; height:0; line-height:0; visibility:hidden;}

.clearit {
	clear: both;   
	font-size: 0;
	line-height: 0;
	height: 0;
}
.clear-both {clear: both;}

.add-bottom-margin {margin-bottom:25px;}



::selection {
	background: #FF4F2C; /* Safari */
	}
::-moz-selection {
	background: #FF4F2C; /* Firefox */
}


/* Layout ----------------------------------------------------------------- */

body {background-color: #ffffff; height:100%; position:relative;
	
 }


/* Typography ----------------------------------------------------------------- */


/* 1em = 10px. Super easy Right? 945px = 94.5em -------------------------------------------- */



body {font-size: 62.5%;/*10px*/ line-height: 1.375em ; color: #333333; font-family: "Avenir LT Std" "soleil";  }

h1 {
	font-size: 5.25em;/*46px*/
	line-height: 1.2em;
	letter-spacing: 1.6;
	font-weight: normal;
	font-family: "Avenir LT Std", Helvetica;
	font-weight: 800;
	margin: 55px auto 32px auto;
	text-align: center;
	}
	
	
h2 {font-size: 3.5em;/*20px*/
	line-height: 1.33em;
	font-weight: normal;
	

	font-family: "Avenir LT Std", Helvetica;
	margin: 0 0 17px 0;

}



h3 {font-size: 2em;/*16px*/
	font-weight: normal;
	letter-spacing: 1.5;	
	font-family: "Avenir LT Std", Helvetica;
	font-weight: 800;


}



h4 {font-size: 1.3125em;/*21px*/ font-weight: normal;  }



h5 {font-family: Helvetica, Arial, sans-serif; font-size: 15px;/*48px*/ color:#888888; font-weight: bold; text-transform: uppercase; letter-spacing: 1px; line-height: 22px;  margin-bottom: 11px;}



p {
	font-size: 1.6em;/*16px*/
	line-height: 120%;
	font-weight: lighter;
	font-family: "Avenir LT Std", Helvetica;	
	}


ul,
ol {margin-bottom: 0px;
	
	margin-bottom: 1.2em;
	
	
}
li {margin-left: 0px;}
a {text-decoration: none; color: #888888;
	
	
	
}
a:hover {color: #FF4F2C; text-decoration: none;}

.caps {text-transform: uppercase; letter-spacing: 0.1em; font-size: .8em;}

.blockQuote{
	
	font-size: 3.3em;/*36px*/
	line-height: 120%;;
	font-weight: normal;
	font-family: "Avenir LT Std", Helvetica;
	margin: 50px 40px;

}



/* Variables ----------------------------------------------------------------- */

$red: #A71D36;
$grey: #333333;
$yellow: #F9D016;
$white: #FFFFFF;




/* Universal ----------------------------------------------------------------- */


#wrapper{

	width: auto;
	margin: 0 auto;	
}

nav{
	width: auto;
	margin: 0 auto !important;
	text-align: center;
	z-index: 4000;
	position: relative;		
}

nav:after {
	position: absolute;
	width: 96em;
	margin: 0 auto;
	display: block;
	position: absolute;
		.navWrapper{
		
			left: 50%;
			margin-left: -48em;	
			width: auto;
			height: 9em;
			background: transparent url('../../images/nav_bg.png') repeat-x;
			z-index: 1000;				
			}
	}	
#content{
		margin:0 auto;

	
}	
	






.navWrapper{
	
	
	margin: 0 auto;
	width: auto;
	display: block;
	height: 9em;
	background: transparent url('../../images/nav_bg.png') repeat-x;
	z-index: 1000;	
}
.float_right{
	float: right;	
}

/* Header ----------------------------------------------------------------- */

header{
	margin: 0;

	height: 637px;
	text-align:center;
	
	
}
.headingWrapper{

	width: auto;
	margin: 0 auto;

		h3{
			font-weight: lighter;
			
			
		}
	
	
}






.splashSlideshow{

	display: block;
	position: absolute;
	width: 96em;
	height: 637px;
	left: 50%;
	margin-left: -48em;
	z-index: 50;

}




.sectionHeader{
	/*background: $red;*/
	color: $white;
	z-index: 50;
	width: 96em;
	height:17.7em;
	
	
	
}

.navlogo{
	float: left;
	
	
}


.splashHeader{
	display: block;
	position: absolute;
	left: 50%;
	margin-left: -48em;	
}

.attributes{
	color: $white;
	position: absolute;
	left: 50%;
	margin-left: -480px;
	top: 200px;	
	
		.blockQuote{
			text-align: left;
			
			
		}
	
	
	
}







.sectionHeaderSub{
		margin: 0 2em 0 0;
		color: $white;
		
		
		
	}




/* Nav ----------------------------------------------------------------- */

.nav{
	display: block;
	font-family: "Avenir LT Std", Helvetica;
	font-size: 1.6em;
	z-index: 1000;
	color: $white;	
	padding: 2em 0;
	margin: 0 auto;
	text-align: center;


	li{
		margin: 0 1.4em;
		
	}
	
	.noMarginLeft{
		margin: 0 1.4em 0 0;
		
		
	}

}




.bgred{
	
	background: $red;
	text-align: center;
	padding: 1.9em .95em ;
	
	
}




/* Main ----------------------------------------------------------------- */

.velo{
	position: absolute;
	left: 50%;
	margin-left: -480px;
	top: 360px;
	
	
}


.aboutHeader{
	display: block;
	height: 18em;
	background: transparent url("../../images/banner1.png") no-repeat 0 0;
	
	
}

.aboutSection{
	
	
	
}


/* --COLLECTION-------------------------------------------------------------------------- */




.collectionSection{


}


.lifestylePic{
		display:block;
		margin: 50px 10px 0 10px;
	}
	
	
.collectionHeader{
	display: block;
	height: 18em;
	background: transparent url("../../images/banner2.png") no-repeat 0 0;

	
	
}
	


	.collectionSubhead{
		display: block;
		float: left;
			h1{text-align: left;
				margin: 50px 0;			
				letter-spacing: 1;
				}
		
	}


.collectionDescription{
		display: block;
		margin: 50px 0 0 10px;
		
		
	}
	
	.collectionImage{
		display: block;
		
	}
	
	.collectionTitle{
		display: block;
		width: 300px;
		margin:0 0 50px 0;
		padding: 10px 10px 0 10px;		
			
	}


/* --CONNECTION-------------------------------------------------------------------------- */



.connectionSection{
	.retailLocations{
		padding: 5em 0;
		}

	.locations{
	font-weight: normal;
	line-height: 200%;
	font-family: "Avenir LT Std", Helvetica;	
	
		ul{
			font-size: 1.6em;/*16px*/
			line-height: 200%;
			font-weight: lighter;
			font-family: "Avenir LT Std", Helvetica;	
			}	
		.contact{
			margin:90px 0 0 0;
				h3{
					margin-bottom: 10px;
					}
			}	
		.facebook{
			border-bottom: .5px solid $grey;
			border-top: .5px solid $grey;
			margin-right:100px;
			padding:10px 0 ;			
			}
		.facebookIcon{
			display: inline-block;
			float: left;
			margin:	0 10px 0 0;
			background: transparent url(images/facebookIcon.jpg) no-repeat 0 0;
			}	
	margin-bottom: 80px;
	}
	.googleMap{
	display: block;
	height: 45em;
	background: #DDD;	
	}
}



.connectionHeader{
		background: transparent url("../../images/banner3.png") no-repeat 0 0;

	
	
	
	
}

/* Footer ----------------------------------------------------------------- */


.footerWrapper{
		width: auto;
		height: 19em;
		background: transparent url('../../images/footer_bg.jpg') repeat-x;

	.footer{
		padding: 46px 0 0 0;
		color: $white;
		
			p{
				margin-top:20px;
				}
		 
		 a{
			 color: $yellow;
		 }
		 
		 a:hover{
			 color: lighten($yellow, 20%);
		 }
	}		
}
bottomFooter{
	margin-top: 5px;
	}

/* ----- JS Styles--------------------------------------------------------- */
.fixed {
    position:fixed;
    }

.background{
	position: fixed;
	width: 100%;
	top: 0;
	left: 0;
	background: transparent url(../../images/nav_bg.png) repeat-x;	
	}

.navGap{
	margin-top: 90px;	
	}
/*---------SLIDESHOW--------------------------------*/



.slide{
	display: block;
	width: auto;
	
	}

#slideshow {
    position:relative;
    height:350px;
    }

#slideshow .slide {
    position:absolute;
    top:0;
    left:0;
    z-index:300;
    }

#slideshow .slide.active {
    z-index:500;
    }

#slideshow .slide.last-active {
    z-index:400;
    }

/*----------Slideshow Text-----------------------------------------------------*/

#slideshow {
    position:relative;
    height:350px;
    }

#slideshow .splashHeader{
    position:absolute;
    top:360px;
    left: 50%;
    margin-left: -480px;
    z-index:305;
    }

#slideshow .splashHeader.active {
    z-index:505;
    }

#slideshow .splashHeader.last-active {
    z-index:405;
    }
#slideshow .attributes{
    position:absolute;
    top:200px;
    left: 50%;
    margin-left: -480px;
    z-index:305;
    }

#slideshow .attributes.active {
    z-index:505;
    }

#slideshow .attributes.last-active {
    z-index:405;
    }


	
	
	
	




